api:
  stockfish:
    key: your_stockfish_api_key_here
    endpoint: https://stockfish.example.com/api/v1
  
  openai:
    key: your_openai_api_key_here
    org_id: your_org_id_here
    model: gpt-4

database:
  main:
    host: localhost
    port: 5432
    name: aeon_chess
    user: aeon_user
    password: your_password_here
  
  redis:
    host: localhost
    port: 6379
    password: your_redis_password_here

auth:
  jwt_secret: your_jwt_secret_here
  encryption_key: your_encryption_key_here
  
services:
  email:
    smtp_host: smtp.example.com
    smtp_port: 587
    username: your_email_here
    password: your_email_password_here
  
  sentry:
    dsn: your_sentry_dsn_here
    environment: development

cloud:
  aws:
    access_key_id: your_aws_access_key_here
    secret_access_key: your_aws_secret_key_here
    region: us-east-1
    bucket: aeon-chess-assets
  
  gcp:
    project_id: your_gcp_project_id_here
    credentials_file: path/to/credentials.json

mobile:
  ios:
    push_cert: path/to/push_cert.pem
    bundle_id: com.aeon.chess
  
  android:
    fcm_key: your_fcm_key_here
    package_name: com.aeon.chess

monitoring:
  datadog:
    api_key: your_datadog_api_key_here
    app_key: your_datadog_app_key_here
    environment: development
