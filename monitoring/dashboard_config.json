{
  "dashboards": [
    {
      "name": "System Overview",
      "panels": [
        "cpu",
        "memory",
        "requests",
        "errors"
      ]
    },
    {
      "name": "Performance Metrics",
      "panels": [
        "latency",
        "throughput",
        "cache_hits"
      ]
    }
  ],
  "alerts": {
    "cpu_high": {
      "threshold": 80,
      "action": "scale_up"
    },
    "memory_high": {
      "threshold": 85,
      "action": "scale_up"
    },
    "error_rate": {
      "threshold": 5,
      "action": "notify"
    }
  }
}