{
  "name": "aeon-chess",
  "version": "1.0.0",
  "description": "AEON Chess - Sistema Cultural de Xadrez com ARKITECT",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "arkitect:analyze": "python3 scripts/arkitect_super_scope_taskmash.py",
    "arkitect:optimize": "node scripts/optimize-bundle.js",
    "arkitect:quality": "python3 scripts/arkitect_quality_gate.py",
    "arkitect:security": "python3 scripts/arkitect_security_scan.py",
    "arkitect:performance": "python3 scripts/arkitect_performance_analysis.py",
    "arkitect:report": "python3 scripts/arkitect_final_report.py",
    "arkitect:full": "npm run arkitect:analyze && npm run arkitect:optimize && npm run arkitect:quality",
    "prebuild": "npm run arkitect:analyze",
    "postbuild": "npm run arkitect:performance"
  },
  "keywords": [
    "chess",
    "ai",
    "cultural",
    "arkitect",
    "enterprise",
    "nextjs",
    "react",
    "typescript"
  ],
  "author": "AEON Chess Team",
  "license": "MIT",
  "dependencies": {
    "@sentry/react": "^10.5.0",
    "@tensorflow/tfjs": "^4.22.0",
    "autoprefixer": "^10.4.16",
    "framer-motion": "^10.16.4",
    "immer": "^10.1.1",
    "mermaid": "^11.9.0",
    "next": "13.4.19",
    "postcss": "^8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "tailwindcss": "^3.3.5",
    "web-vitals": "^5.1.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@types/node": "20.8.2",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.11",
    "eslint": "8.51.0",
    "eslint-config-next": "13.4.19",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "5.2.2"
  },
  "arkitect": {
    "version": "1.0.0",
    "mode": "full_integration",
    "quality_gate": 0.85,
    "performance_target": 0.9,
    "security_target": 0.95,
    "architecture_target": 0.9,
    "auto_optimization": true,
    "continuous_monitoring": true,
    "integration_points": [
      "ci_cd",
      "build_system",
      "testing_framework",
      "monitoring_system",
      "deployment_pipeline"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
